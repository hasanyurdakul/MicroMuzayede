{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ClientApp": "https://app.micromuzayede.com",
  "IdentityServer": {
    "Authority": "http://identity-svc"
  },
  "ReverseProxy": {
    "Clusters": {
      "auctions": {
        "Destinations": {
          "auctionApi": {
            "Address": "http://auction-svc"
          }
        }
      },
      "search": {
        "Destinations": {
          "searchApi": {
            "Address": "http://search-svc"
          }
        }
      },
      "bids": {
        "Destinations": {
          "bidApi": {
            "Address": "http://bid-svc"
          }
        }
      },
      "notifications": {
        "Destinations": {
          "notificationApi": {
            "Address": "http://notification-svc"
          }
        }
      }
    }
  }
}
